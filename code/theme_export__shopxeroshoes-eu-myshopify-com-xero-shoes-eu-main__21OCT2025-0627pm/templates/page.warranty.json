                "text": "<h1>Xero Shoes 5,000 Mile Sole Limited Warranty and 24 Month Manufacturer’s Limited Warranty<\/h1>",
                "text_fallback": "<p>Share information about your brand with your customers.<\/p>",
                "text_size": "",
                "font_family": "font-family-heading",
                "text_style": "",
                "font_weight": "",
                "alignment": "text-md-start",
                "alignment_mobile": "text-start",
                "color": "",
                "bottom_line": false,
                "margin_top_desktop": "mt-md-0",
