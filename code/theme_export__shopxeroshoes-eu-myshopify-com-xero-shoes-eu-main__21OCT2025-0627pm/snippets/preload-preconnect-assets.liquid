{%- comment -%}Pre-connects{%- endcomment -%}
<link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
<link rel="preconnect" href="fluid-framework-2-0.myshopify.com/cdn/shop/" crossorigin>
<link rel="preconnect" href="https://monorail-edge.shopifysvc.com" crossorigin>
{{ 'paralucent-light-webfont.woff2' | asset_url | preload_tag: as: 'font', type: 'font/woff2', crossorigin: 'anonymous' }}
{{ 'paralucent-lightitalic-webfont.woff2' | asset_url | preload_tag: as: 'font', type: 'font/woff2', crossorigin: 'anonymous' }}
{{ 'paralucent-medium-webfont.woff2' | asset_url | preload_tag: as: 'font', type: 'font/woff2', crossorigin: 'anonymous' }}
{{ 'paralucent-demibold-webfont.woff2' | asset_url | preload_tag: as: 'font', type: 'font/woff2', crossorigin: 'anonymous' }}
{{ 'paralucent-bold-webfont.woff2' | asset_url | preload_tag: as: 'font', type: 'font/woff2', crossorigin: 'anonymous' }}
{{ 'inter-v19-latin-regular.woff2' | asset_url | preload_tag: as: 'font', type: 'font/woff2', crossorigin: 'anonymous' }}
{{ 'inter-v19-latin-italic.woff2' | asset_url | preload_tag: as: 'font', type: 'font/woff2', crossorigin: 'anonymous' }}
{{ 'inter-v19-latin-500.woff2' | asset_url | preload_tag: as: 'font', type: 'font/woff2', crossorigin: 'anonymous' }}
{{ 'inter-v19-latin-600.woff2' | asset_url | preload_tag: as: 'font', type: 'font/woff2', crossorigin: 'anonymous' }}
{{ 'global.css' | asset_url | preload_tag: as: 'style' }}
{{ 'utilities.css' | asset_url | preload_tag: as: 'style' }}
{%- if template == 'collection' -%}
    {{ 'component-filter.css' | asset_url | preload_tag: as: 'style' }}
{%- endif -%}
{%- if template == 'collection' and collection.products[0].featured_image -%}
    {{ collection.products[0].featured_image | image_url | preload_tag: as: image }}
{%- elsif template == 'product' and product.featured_image -%}
    {{ product.featured_image | image_url | preload_tag: as: image }}
{%- endif -%}